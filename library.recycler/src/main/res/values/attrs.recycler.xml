<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <!-- /SVIOLET.RECYCLER/src/sviolet/liba/view/slidelist/SlideListItem.java -->
    <declare-styleable name="SlideListItem">
        <attr name="SlideListItem_slideRange" format="dimension" /><!-- 侧滑距离 -->
        <attr name="SlideListItem_shadowWidth" format="float" /><!-- 纵向阴影宽度 -->
        <attr name="SlideListItem_flipSpeed" format="float" /><!-- 侧滑速度 -->
        <attr name="SlideListItem_containerFlipSpeed" format="float" /><!-- 容器缩放速度 -->
        <attr name="SlideListItem_flipAcceleration" format="float" /><!-- 侧滑加速度 -->
        <attr name="SlideListItem_containerFlipAcceleration" format="float" /><!-- 容器缩放加速度 -->
        <attr name="SlideListItem_upMoveable" format="boolean" /><!-- 上层View可滑动 -->
        <attr name="SlideListItem_downMoveable" format="boolean" /><!-- 下层View可滑动 -->
        <attr name="abordTouchEvent" /><!-- 阻断触摸事件(不向下分发) -->
	</declare-styleable>

    <!-- /SVIOLET.RECYCLER/src/sviolet/liba/view/AsyncImageView.java -->
    <declare-styleable name="AsyncImageView">
        <attr name="src" /><!-- 图片资源ID -->
        <attr name="colour"/><!-- 颜色 -->
    </declare-styleable>
    
</resources>
