<!-- 圆角 灰色 按下深灰 -->
<!-- 中奖纪录 地址录入 listView Item项使用 -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:drawable="@drawable/rounded_list_item_pressed" android:state_pressed="true"/>
    <item android:drawable="@drawable/rounded_list_item_pressed" android:state_focused="true"/>
    <item android:drawable="@drawable/rounded_list_item_pressed" android:state_checked="true"/>
    <item android:drawable="@drawable/rounded_list_item_normal"/>

</selector>